{
  "compilerOptions": {
    "target": "esnext",

    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "useDefineForClassFields": true,

    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,

    "lib": ["esnext", "dom"]
  },
  "include": [".vitepress/**/*", ".vitepress/**/*.vue"]
}
